        <![CDATA[
        SELECT count(1)FROM gift g, (
                    SELECT mid.*,ui2.user_name rechargeUserName,ui2.head_photo_url rechargePhotoUrl
                     FROM (SELECT
                              ug.*,
                              ui.user_name      userName,
                              ui.head_photo_url userPhotoUrl
                            FROM user_gift ug, user_info ui
                            WHERE ug.gift_sender = ui.user_id
                          ) mid, user_info ui2
                     WHERE mid.gift_receiver = ui2.user_id
         ]]>
        <if test="searchTxt!=null">
           and (userName LIKE #{searchTxt} OR ui2.user_name LIKE #{searchTxt})
        </if>
        <![CDATA[
             AND(mid.created_time <= #{searchTime} OR mid.created_time IS NULL)
           ) mmid where g.gift_id=mmid.gift_id
         ]]>
    </select>

